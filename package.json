{
  "name": "eat-cli",
  "version": "0.1.0",
  "description": "Eat - The Bun Control Plane. Orchestrate Bun projects with opinionated workflows.",
  "type": "module",
  "bin": {
    "eat": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "bun run src/cli.ts",
    "build": "bun build src/cli.ts --outdir dist --target bun --minify",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build && bun test"
  },
  "keywords": [
    "bun",
    "workflow",
    "orchestration",
    "monorepo",
    "developer-tools",
    "cli",
    "build-tool",
    "task-runner",
    "bun-native"
  ],
  "author": "Eat Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Elepcode/eat.git"
  },
  "bugs": {
    "url": "https://github.com/Elepcode/eat/issues"
  },
  "homepage": "https://github.com/Elepcode/eat#readme",
  "dependencies": {
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "bun": ">=1.0.0"
  },
  "engines": {
    "bun": ">=1.0.0",
    "node": ">=18.0.0"
  }
}